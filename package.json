{
  "name": "raidleader.io",
  "version": "1.0.0",
  "description": "Express SSR React App for raidleader.io.",
  "main": "server/index.js",
  "repository": "git@github.com:krnl-panic/rlio-react.git",
  "author": "Michael Fay <mike@michaelfay.io>",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "scripts": {
    "start": "rm -rf ./dist/* && webpack --mode=development --config webpack/dev/webpack.dev.server.js --config webpack/dev/webpack.dev.client.js && node dist/server.js",
    "dev": "npm-run-all --parallel startWatch:*",
    "startWatch:server": "nodemon -w \"./dist\" -x \"node dist/server.js\"",
    "startWatch:build-server": "webpack --config webpack/dev/webpack.dev.server.js --watch",
    "startWatch:build-client": "webpack --config  webpack/dev/webpack.dev.client.js --watch --no-cache",
    "build:prod": "rm -rf ./dist/* && webpack --config webpack/prod/webpack.prod.server.js --config webpack/prod/webpack.prod.client.js",
    "build:dev": "rm -rf ./dist/* && webpack --config webpack/dev/webpack.dev.server.js --config webpack/dev/webpack.dev.client.js"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@fontsource/roboto": "^4.5.5",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/component": "^5.15.2",
    "@loadable/server": "^5.15.2",
    "@loadable/webpack-plugin": "^5.15.2",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "antd": "^4.20.0",
    "babel-loader": "^8.2.5",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-is": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "react-social-login-buttons": "^3.6.0",
    "styled-components": "^5.3.5",
    "terser-webpack-plugin": "^5.3.1",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/loadable__component": "^5.13.4",
    "@types/loadable__server": "^5.12.6",
    "@types/lodash": "^4.14.182",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-middleware": "^5.3.0",
    "@types/webpack-env": "^1.16.4",
    "@types/webpack-hot-middleware": "^2.25.6",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5"
  }
}
